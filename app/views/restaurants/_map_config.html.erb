<% if @restaurant %>
<div id="map-config" data-token="<%=ENV['mapbox_access_token']%>"
                     data-style="<%=ENV['map_style']%>"
                     data-google-key="<%=ENV['google_geolocation_key']%>"
                     data-current-lat="<%=@restaurant.latitude%>"
                     data-current-lng="<%=@restaurant.longitude%>">
</div>
<% end %>