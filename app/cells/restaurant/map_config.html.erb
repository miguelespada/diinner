<% if model %>
<div id="map-config" data-token="#{ENV['mapbox_access_token']}"
data-style="#{AdminSettings.get_first.map_box}"
                     data-google-key="#{ENV['google_geolocation_key']}"
                     data-current-lat="#{model.latitude}"
                     data-current-lng="#{model.longitude}">
</div>
<% end %>